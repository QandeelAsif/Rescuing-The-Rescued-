<% include ../partials/header.ejs %>
<!-- CASE DETAILS -->
<div class="container">
    <h4>Add Case Details</h4><br />
    <form id="case-form">
        <div class="input-field">
            <input type="text" id="child-name" placeholder="Enter name" name="name" required />
        </div>
        <div class="input-field">
            <input type="number" id="child-age" placeholder="Enter age" name="age" required />
        </div>
        <div class="input-field">
            <input type="text" class="date-picker" id="child-dob" placeholder="Enter Date Of Birth" name="dob"
                required />
        </div>
        <div class="input-field">
            <input type="text" id="child-city" placeholder="Enter City " name="city" required />
        </div>
        <div class="input-field">
            <input type="text" id="child-description" placeholder="Enter Description(if any)" name="description" />
        </div>
        <button type="submit" waves-effect waves-light btn red"> Save</button>
    </form>
</div>
<% include ../partials/footer.ejs %>
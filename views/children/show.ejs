<div class="container data-wrapper text-align:center-align">



        <ul class="collection with-header">
                <li class="collection-header">
                        <div class="row">
                                <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"><span> </span></div>
                                <div class="col l6 m6 s5 data-text-div"><span id="data-child-name"></span><br></div>
                            </div><h4 style="color: brown"><%=name%></h4></li>
                <li class="collection-item">
                        <div class="row">
                                <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"><span> <em>CHILD AGE:</em> <%=age%></span></div>
                                <div class="col l6 m6 s5 data-text-div"><span id="data-child-age"></span><br></div>
                            </div>
                </li>
                <li class="collection-item"><div class="row">
                        <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"> <em>CHILD DOB :</em> <span><%=dob%></span></div>
                        <div class="col l6 m6 s5 data-text-div"><span id="data-child-dob"></span><br></div>
                    </div></li>
                <li class="collection-item"><div class="row">
                        <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"><span>CHILD CITY: <%=city%></span></div>
                        <div class="col l6 m6 s5 data-text-div"><span id="data-child-city"></span><br></div>
                    </div></li>
                <li class="collection-item"><div class="row">
                        <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"><span>SUMMARY :<%=desc%></span></div>
                        <div class="col l5 m6 s5 data-text-div"><span id="data-child-summary"></span><br></div>
                    </div></li>
                    <li class="collection-item">
                            <div class="row thumbnail">
                                    <div class="col l2 m3 s3 offset-l2 offset-m1 offset-s1 data-div"><img class="responsive-img"src="<%=url%>" alt=""></div>
                            </div>
                    </li>
              </ul>

